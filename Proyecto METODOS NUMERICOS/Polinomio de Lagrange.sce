function btnGenerar_callback(handles)
    //%e^(2*x)*cos(3*x)
    //0,0.3,0.6
    funcion =handles.inFuncion.string 
    deff('y=g(x)',strcat(['y=',funcion]));
    puntos = handles.inPuntos.string;
    puntos = strcat(['[',puntos,']']);
    puntos = evstr(puntos);
    lista= string([1 puntos(1) g(puntos(1))]);
    for i=2 : length(puntos)
        lista=cat(1,lista,string([i puntos(i) g(puntos(i))]));
    end
    titulo = ["n" "x" "y"];
    datosTabla = cat(1,titulo,lista);
    handles.tabla.string = datosTabla;
    lista = evstr(lista);
    disp(lista(2,3));
    n=size(lista, "r");
    disp(n);
    poli="";
    for i=1 : n
        y = lista(i,3);
        protagonista=string(lista(i,2));
        lagrange= "";
        for j=1: n
            if i~=j
                xv= lista(j,2);
                lagrange = strcat([lagrange,'*(x-',string(xv),')/(',protagonista,'-',string(xv),')']);
            end;
        end;
        poli = strcat([poli,'+',string(y),lagrange]);
    end;
    poliLagrange = strcat(['y=',poli]);
    deff('y=p(x)',poliLagrange);
    graf=lista(1,2)-1:0.1:lista(n,2)+1;
    plot(graf,p,'r');
    plot(graf,g);
    [q,w,e]=string(g)
    x=poly(0,'x');
    poli=evstr(poli);
    disp(prettyprint(poli))
    xtitle(prettyprint(poli),e,"y");
    x = lista(:,2);0
    y = lista(:,3);
    scatter(x, y);
    //%e^(2*x)*cos(3*x)
    //0,0.3,0.6
    evalPunto=evstr(handles.inEval.string);
    errorEval = abs(g(evalPunto)-p(evalPunto));
    handles.evalFText.string=strcat(['f(',string(evalPunto),')=',string(g(evalPunto))]);
    handles.evalPTExt.string=strcat(['P',string(n-1),'(',string(evalPunto),')=',string(p(evalPunto))]);
    handles.errorText.string=strcat(['|P',string(n-1),'(',string(evalPunto),')-f(',string(evalPunto),')|=',string(errorEval)]);
    
    
endfunction


